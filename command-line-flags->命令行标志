  <script>
      onkeydown = (e) => {
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'command-line-arguments->命令行参数';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'command-line-subcommands->命令行子命令';
          }
          
      }
  </script>
