  <script>
      onkeydown = (e) => {
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'writing-files->写文件';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'file-paths->文件路径';
          }
          
      }
  </script>
